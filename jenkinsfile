 pipeline {
    
 
    agent any

		stages 
		{
 	
			stage('SCM Checkout') 
			{
				steps 
				{
               
               
				git "https://github.com/TanumayDhar/greenkart6-.git"
                
				}
			}
			stage('Build') 
			{
				steps 
				{
             
          
       			echo 'Building the application...'
                
                
				}
			}
			stage('Testing with Cucumber') 
			{
				steps 
				{
            
				echo 'Test and Deploying in server...'
				bat 'mvn test -Dtest=runner test'
            
				}
			}
		}
}
 
 
